{
  "version": "1.2.1",
  "npmClient": "yarn",
  "loglevel": "info",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "conventionalCommits": true
    },
    "version": {
      "conventionalCommits": true,
      "createRelease": "github",
      "message": "chore(release): publish new version %v",
      "gitDryRun": false
    }
  }
}